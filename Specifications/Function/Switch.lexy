function BooleanSwitch
  Parameters
    boolean Evil
  Results
    number Number
  Code
    number temp = 555
    switch Evil
      case true
        temp = 666
      case false
        temp = 777
    Number = temp

scenario BooleanSwitchTrue
  function BooleanSwitch
  Parameters
    Evil = true
  Results
    Number = 666

scenario BooleanSwitchFalse
  function BooleanSwitch
  Parameters
    Evil = false
  Results
    Number = 777



function NumberSwitch
  Parameters
    number Evil
  Results
    number Number
  Code
    number temp = 555
    switch Evil
      case 6
        temp = 666
      case 7
        temp = 777
      default
        temp = 888
    Number = temp

scenario NumberSwitchCase
  function NumberSwitch
  Parameters
    Evil = 7
  Results
    Number = 777

scenario NumberSwitchDefault
  function NumberSwitch
  Parameters
    Evil = 9
  Results
    Number = 888




function StringSwitch
  Parameters
    string Evil
  Results
