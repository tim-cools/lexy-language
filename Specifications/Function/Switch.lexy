enum SwitchValue
  Holy
  Evil
  Unknown

function EnumSwitch
  Parameters
    SwitchValue Level
  Results
    string Number
  Code
    string temp = "999"
    switch Level
      case SwitchValue.Evil
        temp = "666"
      case SwitchValue.Holy
        temp = "777"
      default
        temp = "888"
    Number = temp

scenario EnumSwitchCaseHoly
  function EnumSwitch
  Parameters
    Level = SwitchValue.Holy
  Results
    Number = "777"

scenario EnumSwitchCaseEvil
  function EnumSwitch
  Parameters
    Level = SwitchValue.Evil
  Results
    Number = "666"

scenario EnumSwitchDefault
  function EnumSwitch
  Parameters
    Level = SwitchValue.Unknown
  Results
    Number = "888"




function EnumNestedSwitch
  Parameters
    boolean Evil
    SwitchValue Level
  Results
    string Number
  Code
    string temp = "999"
    switch Level
      case SwitchValue.Evil
        if Evil
          temp = "666"
        else
          temp = "555"
      case SwitchValue.Holy
        temp = "777"
      default
        temp = "888"
    Number = temp

scenario EnumNestedSwitchIfFalse
  function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = false
  Results
    Number = "555"

scenario EnumNestedSwitchIfTrue
  function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = true
  Results
    Number = "666"
