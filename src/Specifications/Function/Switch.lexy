Function: EnumNestedSwitch
  Parameters
    boolean Evil
    SwitchValue Level
  Results
    string Number
  Code
    string temp = "999"
    switch Level
      case SwitchValue.Evil
        if Evil
          temp = "666"
        else
          temp = "555"
      case SwitchValue.Holy
        temp = "777"
      default
        temp = "888"
    Number = temp

Scenario: EnumNestedSwitchIfFalse
  Function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = false
  Results
    Number = "555"

Scenario: EnumNestedSwitchIfTrue
  Function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = true
  Results
    Number = "666"
