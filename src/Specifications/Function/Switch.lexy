Function: BooleanSwitch
  Parameters
    boolean Evil
  Results
    number Number
  Code
    number temp = 555
    switch Evil
      case true
        temp = 666
      case false
        temp = 777
    Number = temp

Scenario: BooleanSwitchTrue
  Function BooleanSwitch
  Parameters
    Evil = true
  Results
    Number = 666

Scenario: BooleanSwitchFalse
  Function BooleanSwitch
  Parameters
    Evil = false
  Results
    Number = 777



Function: NumberSwitch
  Parameters
    number Evil
  Results
    number Number
  Code
    number temp = 555
    switch Evil
      case 6
        temp = 666
      case 7
        temp = 777
      default
        temp = 888
    Number = temp

Scenario: NumberSwitchCase
  Function NumberSwitch
  Parameters
    Evil = 7
  Results
    Number = 777

Scenario: NumberSwitchDefault
  Function NumberSwitch
  Parameters
    Evil = 9
  Results
